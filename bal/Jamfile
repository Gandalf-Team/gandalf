SubDir GD_TOP bal ;

GdBalSources
    gio.c
    mmap.c
    ;

GdIoctls libgdioctl.a : [ FDirName $(GD_TOP) bal ioctl.txt ] ;

# Include arch, soc, platform headers
SubIncludeOnce GD_TOP bal arch $(ARCH) ;
if $(SOC) {
    SubIncludeOnce GD_TOP bal soc $(SOC) ;
}
SubIncludeOnce GD_TOP bal platform $(PLATFORM) ;

SubDir GD_TOP bal ;

GdBalBinaries ;